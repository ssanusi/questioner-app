{
  "name": "questioner-api",
  "version": "1.0.0",
  "description": "Restful api for questioner app",
  "main": "index.js",
  "scripts": {
    "db:create:dev": "createdb questionerdb_dev",
    "db:create:test": "createdb questionerdb_test",
    "db:init:dev": "psql -d questionerdb_dev -f server/src/db/schema.sql",
    "db:init:test": "psql -d questionerdb_test -f server/src/db/schema.sql",
    "db:migrate": "psql -d questionerdb_dev -f server/src/db/seed.sql",
    "db:migrate:test": "psql -d questionerdb_test -f server/src/db/seed.sql",
    "build": "npm run build:server && npm run build:client",
    "build:server": "babel server/src -d server/dist",
    "build:client": "webpack --config webpack-client.config.js --mode production",
    "start": "npm run build && node server/dist/index.js",
    "dev": "webpack-dev-server --config webpack-client.config.js --mode development --open --hot",
    "serve": "node server/dist/index.js",
    "test": "npm run test:server && npm run test:client",
    "test:server:dev": "npm run db:init:dev && npm run db:migrate && nyc mocha server/test --require @babel/register  --timeout 200000  --exit",
    "test:server": "npm run db:init:test && npm run db:migrate:test && nyc mocha server/test --require @babel/register --exit",
    "test:client": "jest client/",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "deploy": "git subtree push --prefix UI origin gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ssanusi/questioner-api.git"
  },
  "author": "Sulaiman Sanusi",
  "license": "MIT",
  "homepage": "https://ssanusi.github.io/questioner-app",
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "formik": "^1.5.7",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.13",
    "lodash.isempty": "^4.4.0",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "pg": "^7.11.0",
    "pg-promise": "^8.7.2",
    "swagger-ui-express": "^4.0.5",
    "validator": "^11.0.0",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "@testing-library/react": "^8.0.1",
    "@types/jest": "^24.0.12",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "bootstrap": "^4.3.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "copy-webpack-plugin": "^5.0.3",
    "coveralls": "^3.0.3",
    "css-loader": "^2.1.1",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "7.13.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "jest-dom": "^3.4.0",
    "lodash.throttle": "^4.1.1",
    "mini-css-extract-plugin": "^0.7.0",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "node-gyp": "^4.0.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "prettier": "^1.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "react-testing-library": "^7.0.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "sweetalert": "^2.1.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-node-externals": "^1.7.2",
    "webpack-suit-up": "^1.0.0"
  },
  "bugs": {
    "url": "https://github.com/ssanusi/questioner-api/issues"
  },
  "jest": {
    "roots": [
      "./client"
    ],
    "moduleNameMapper": {
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js",
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/client/__test__/__mocks__/"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
